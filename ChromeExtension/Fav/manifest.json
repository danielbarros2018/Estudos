{
  "manifest_version": 3,
  "name": "Favs",
  "author": "Daniel Barros",
  "description": "Salvar os links de forma organizada.",
  "version": "0.0.2",
  "action": {
    "default_icon": "assets/icon.png",
    "default_popup": "index.html"
  },
  "permissions": [
    "tabs",
    "storage",
    "tabGroups"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}